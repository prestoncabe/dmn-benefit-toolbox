      },
      "id": "Field_results",
      "verticalAlignment": "end",
      "conditional": {
        "hide": "=form.incomplete"
      }
    },
    {
      "label": "=if false then\n  \"Placeholder for the results returned from the DMN API. (not meant to be displayed in the deployed form)\"\nelse\n  \"\"",
      "type": "checkbox",
      "layout": {
        "row": "Row_1htq3tz",
        "columns": null
      },
      "id": "Field_benefits-readonly",
      "key": "benefits",
      "readonly": "true",
